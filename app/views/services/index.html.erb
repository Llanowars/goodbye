<h1>Listing Services</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Company</th>
      <th>Street address</th>
      <th>Zip code</th>
      <th>City</th>
      <th>Postal box</th>
      <th colspan="3"></th>
    </tr>
  </thead>
<br>
  <tbody>
    <% @services.each do |service| %>
      <tr>
        <td><%= service.name %></td>
        <td><%= service.company %></td>
        <td><%= service.street_address %></td>
        <td><%= service.zip_code %></td>
        <td><%= service.city %></td>
        <td><%= service.postal_box %></td>
        <td><%= link_to 'Show', service %></td>
      </tr>
    <% end %>
  </tbody>
</table>

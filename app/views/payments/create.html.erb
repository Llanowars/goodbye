<div class="wrapper min-height-screen bg-grey-light thanku-page" style="background-image: linear-gradient(-225deg, rgba(204, 238, 235, 0.74) 32%, rgba(204, 238, 235, 0.43) 50%),  url('/assets/sky-dfb8e451d6ff165980d2f531eb2d9ea44c514ee48b2f99ca698ddeb7949fed29.jpg')">
  <div class="container">
    <div class="row">
      <div class="col-lg-8 col-lg-offset-2 col-md-12">
        <div class="card-thanku">
          <div class="row">
            <div class="col-lg-3 col-sm-12">
              <img src="<%= image_path('icon-thanku2.png') %>" alt="">
            </div>
            <div class="col-lg-9 col-sm-12">
              <div class="card-thanku-message">
                <h1 class="tx-blue">Merci !</h1>
                <h4 class="tx-grey">L'équipe Fleeme s'occupe de tout et vous prévient dès que votre demande de résiliation est transmise à  <%=@unsub.service.name%>. </h4>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col-lg-12">
              <div class="share-row text-center">
                <h4 class="tx-grey">Partagez Fleeme pour en faire profiter vos proches</h4>
                <div class="fb-share-button" data-href="http://www.fleeme.fr" data-layout="button" data-mobile-iframe="true"></div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<div id="fb-root"></div>
<script>(function(d, s, id) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
  js.src = "//connect.facebook.net/fr_FR/sdk.js#xfbml=1&version=v2.6&appId=1183544908331736";
  fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));</script>

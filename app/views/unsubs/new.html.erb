<div class="container text-center">
  <button type="button" class="btn btn-success" id="start-button">Start</button>
</div>
<div class="container">
  <%= form_tag(service_unsubs_path)  do %>
    <% @hash_service.each do |d| %>
      <fieldset>
        <%= label_tag(d[:label], d[:label]) %>
        <% if  d[:type] == "radio"%>
          <% d[:choices].each do |c| %>
            <%= content_tag(:input, c, type: d[:type], name: "unsub_hash[#{d[:name]}]", class: "form-control") %>
          <% end %>
        <%else %>
          <%= content_tag(:input, nil, type: d[:type], name: "unsub_hash[#{d[:name]}]", class: "bg-red") %>
        <% end %>
      </fieldset>
    <% end %>



    <%= submit_tag "validate" %>
  <% end %>
</div>


